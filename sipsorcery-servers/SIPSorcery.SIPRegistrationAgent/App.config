<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net"/>
    <section name="sipregistrationagent" type="SIPSorcery.SIPRegistrationAgent.SIPRegAgentState, sipsorcery-sipregagent"/>
  </configSections>
  <log4net>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%m%n"/>
      </layout>
    </appender>
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender,log4net">
      <param name="File" value="root.log"/>
      <param name="AppendToFile" value="false"/>
      <param name="MaxSizeRollBackups" value="3"/>
      <param name="MaximumFileSize" value="500kB"/>
      <param name="RollingStyle" value="Size"/>
      <param name="StaticLogFileName" value="true"/>
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%d [%t] %-5p %c [%x] - %m%n"/>
      </layout>
    </appender>
    <appender name="ErrorRollingLogFileAppender" type="log4net.Appender.RollingFileAppender,log4net">
      <param name="File" value="error.log"/>
      <param name="AppendToFile" value="false"/>
      <param name="MaxSizeRollBackups" value="5"/>
      <param name="MaximumFileSize" value="1MB"/>
      <param name="RollingStyle" value="Size"/>
      <param name="StaticLogFileName" value="true"/>
      <filter type="log4net.Filter.LevelMatchFilter">
        <param name="LevelToMatch" value="ERROR"/>
      </filter>
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%d [%t] %-5p %c [%x] - %m%n"/>
      </layout>
    </appender>
    <root>
      <priority value="DEBUG"/>
      <appender-ref ref="ConsoleAppender"/>
      <appender-ref ref="RollingLogFileAppender"/>
      <appender-ref ref="ErrorRollingLogFileAppender"/>
    </root>
  </log4net>
  <appSettings>
    <add key="EncryptedSettingsCertificateName" value="www.sipsorcery.com"/>
    <add key="PersistenceStorageType" value="SQLLinqMSSQL"/>
    <add key="PersistenceConnStr" value=""/>
  </appSettings>
  <sipregistrationagent>
    <MonitorLoopbackPort value="10001"/>
    <OutboundProxy value="10.1.1.5:5060"/>
    <sipsockets>
      <sipsocket>10.1.1.5:5004</sipsocket>
    </sipsockets>
  </sipregistrationagent>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6"/></startup></configuration>
